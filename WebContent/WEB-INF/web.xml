<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>sus_online</display-name>
  <welcome-file-list>
    <welcome-file>view/index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>mapServelet</servlet-name>
    <servlet-class>br.com.sus_online.controller.InicialController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>mapServelet</servlet-name>
    <url-pattern>/Login.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>mapServelet2</servlet-name>
    <servlet-class>br.com.sus_online.controller.AgendaController</servlet-class>
  </servlet>
   <servlet-mapping>
    <servlet-name>mapServelet2</servlet-name>
    <url-pattern>/agendarConsulta</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>mapServelet3</servlet-name>
    <servlet-class>br.com.sus_online.controller.AjaxApp</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>mapServelet3</servlet-name>
    <url-pattern>/ajaxApp</url-pattern>
  </servlet-mapping>
 
</web-app>